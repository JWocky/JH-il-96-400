<?xml version="1.0"?>

<PropertyList>

	<enable3d>true</enable3d>

	<label>
		<name>G Load</name>
		<width>1</width>
		<height>1</height>
		<x>-80</x>
		<y>60</y>
		<input>
			<property>/accelerations/pilot/z-accel-fps_sec</property>
			<factor>-0.03108095</factor>
			<damp>1.0</damp>
		</input>
		<format>G%2.1f</format>
		<halign>left</halign>
	</label>

	<tape>
		<name>Gyrocompass</name>
		<x>-50</x>
		<y>45</y>
		<width>100</width>
		<height>2</height>
		<input>
			<property>orientation/heading-deg</property>
			<factor>0.1</factor>
			<max>85</max>
			<min>0</min>
		</input>
		<display-span>5</display-span>
		<option>bottom</option>
		<major-divisions>5</major-divisions>
		<minor-divisions>1</minor-divisions>
		<tick-bottom>false</tick-bottom>
		<tick-top>false</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>true</cap-bottom>
		<cap-top>true</cap-top>
		<cap-right>true</cap-right>
		<cap-left>true</cap-left>
		<marker-offset>0</marker-offset>
		<enable-pointer>false</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<tick-type>line</tick-type>
		<tick-length>fixed</tick-length>
	</tape>

	<label>
		<name>Gyro Heading Marker</name>
		<x>0</x>
		<y>55</y>
		<width>1</width>
		<height>1</height>
		<format>v</format>
	</label>

	<label>
		<name>Gyro Heading Label</name>
		<x>-10</x>
		<y>60</y>
		<width>17</width>
		<height>4</height>
		<input>
			<property>/orientation/heading-deg</property>
		</input>
		<format>%4.0f</format>
		<halign>center</halign>
		<box>false</box>
	</label>

	<gauge>
		<name>Throttle</name>
		<x>-100</x>
		<y>-60</y>
		<width>5</width>
		<height>100</height>
		<option>vertical</option>
		<option>left</option>
		<option>notext</option>
		<major-divisions>50</major-divisions>
		<minor-divisions>0</minor-divisions>
		<tick-bottom>true</tick-bottom>
		<tick-top>true</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>false</cap-bottom>
		<cap-top>false</cap-top>
		<cap-right>true</cap-right>
		<cap-left>false</cap-left>
		<marker-offset>0.0</marker-offset>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<input>
			<property>/controls/engines/engine/throttle</property>
			<max>1.0</max>
			<min>0.0</min>
		</input>
	</gauge>

	<tape>
		<name>KIAS</name>
		<x>-70</x>
		<y>-60</y>
		<width>10</width>
		<height>100</height>
		<input>
			<property>/velocities/airspeed-kt</property>
			<factor>1</factor>
			<max>850</max>
			<min>-50</min>
		</input>
		<major-divisions>1</major-divisions>
		<minor-divisions>0.1</minor-divisions>
		<display-span>2.0</display-span>
		<option>left</option>
		<option>vertical</option>
		<tick-bottom>false</tick-bottom>
		<tick-top>false</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>false</cap-bottom>
		<cap-top>false</cap-top>
		<cap-right>false</cap-right>
		<cap-left>false</cap-left>
		<marker-offset>5</marker-offset>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
	</tape>

	<label>
		<name>MACH_TXT</name>
		<width>1</width>
		<height>1</height>
		<x>-95</x>
		<y>-70</y>
		<format>MACH:</format>
		<halign>left</halign>
	</label>

	<label>
		<name>MACH_NUM</name>
		<width>1</width>
		<height>1</height>
		<x>-40</x>
		<y>-70</y>
		<input>
			<property>/velocities/mach</property>
		</input>
		<format>%4.1f</format>
		<halign>right</halign>
	</label>

	<label>
		<name>KIAS_TXT</name>
		<width>1</width>
		<height>1</height>
		<x>-95</x>
		<y>-80</y>
		<format>KIAS:</format>
		<halign>left</halign>
	</label>

	<label>
		<name>KIAS_NUM</name>
		<width>1</width>
		<height>1</height>
		<x>-40</x>
		<y>-80</y>
		<input>
			<property>/velocities/airspeed-kt</property>
		</input>
			<format>%3dkts</format>
		<halign>right</halign>
	</label>

	<label>
		<name>GroundSpeed_TXT</name>
		<width>1</width>
		<height>1</height>
		<x>-95</x>
		<y>-90</y>
		<format>GS:</format>
		<halign>left</halign>
	</label>

	<label>
		<name>GroundSpeed_NUM</name>
		<width>1</width>
		<height>1</height>
		<x>-40</x>
		<y>-90</y>
		<format>%3dkts</format>
		<halign>right</halign>
		<input>
			<property>/instrumentation/gps/indicated-ground-speed-kt</property>
		</input>
	</label>

	<tape>
		<name>Altitude-MSL</name>
		<x>60</x>
		<y>-60</y>
		<width>10</width>
		<height>100</height>
		<option>right</option>
		<option>vertical</option>
		<major-divisions>100</major-divisions>
		<minor-divisions>25</minor-divisions>
		<display-span>250.0</display-span>
		<tick-bottom>true</tick-bottom>
		<tick-top>true</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>false</cap-bottom>
		<cap-top>false</cap-top>
		<cap-right>true</cap-right>
		<cap-left>false</cap-left>
		<marker-offset>5</marker-offset>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<tick-type>line</tick-type>
		<tick-length>variable</tick-length>
		<input>
			<property>/position/altitude-ft</property>
		</input>
	</tape>

	<label>
		<name>Altitude_TXT</name>
		<x>20</x>
		<y>-70</y>
		<width>1</width>
		<height>1</height>
		<format>ALT-MSL:</format>
		<halign>left</halign>
	</label>

	<label>
		<name>Altitude_NUM</name>
		<x>100</x>
		<y>-70</y>
		<width>1</width>
		<height>1</height>
		<input>
			<property>/position/altitude-ft</property>
		</input>
		<format>%4.0fft</format>
		<halign>right</halign>
		<box>false</box>
		<option>left</option>
	</label>

	<label>
		<name>AGL_TXT</name>
		<x>20</x>
		<y>-80</y>
		<width>1</width>
		<height>1</height>
		<format>ALT-AGL:</format>
		<halign>left</halign>
	</label>

	<label>
		<name>AGL_NUM</name>
		<x>100</x>
		<y>-80</y>
		<width>1</width>
		<height>1</height>
		<input>
			<property>/position/altitude-agl-ft</property>
		</input>
		<format>%4.0fft</format>
		<halign>right</halign>
		<box>false</box>
		<option>left</option>
	</label>

	<ladder>
		<name>Pitch Ladder</name>
		<type>pitch</type>
		<x>-23</x>
		<y>-320</y>
		<width>48</width>
		<height>620</height>
		<roll-input>
			<property>/orientation/roll-deg</property>
		</roll-input>
		<pitch-input>
			<property>/orientation/pitch-deg</property>
		</pitch-input>
		<compression-factor>7.8</compression-factor>
		<display-span>8.0</display-span>
		<divisions>2.0</divisions>
		<screen-hole>10</screen-hole>
		<zero-bar-overlength>15</zero-bar-overlength>
		<enable-dynamic-origin>false</enable-dynamic-origin>
		<enable-fuselage-ref-line>true</enable-fuselage-ref-line>
		<enable-target-spot>true</enable-target-spot>
		<enable-glide-slope-marker>false</enable-glide-slope-marker>
		<glide-slope>0.0</glide-slope>
		<enable-waypoint-marker>false</enable-waypoint-marker>
	</ladder>

	<label>
		<x>-60</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>WLVL</format>
		<halign>left</halign>
		<condition>
			<equals>
				<property>/autopilot/locks/heading</property>
				<value>wing-leveler</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>-60</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>RH:%3d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/target-roll-deg</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/heading</property>
				<value>roll-hold</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>-60</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>HDG:%3d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/heading-bug-deg</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/heading</property>
				<value>dg-heading-hold</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>-60</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>TRU:%3d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/true-heading-deg</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/heading</property>
				<value>true-heading-hold</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>-60</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>NAV1</format>
		<halign>left</halign>
		<!-- input>
			<property>/autopilot/settings/true-heading-deg</property>
		</input -->
		<condition>
			<equals>
				<property>/autopilot/locks/heading</property>
				<value>nav1-hold</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>-20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>KwT:%3d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/target-speed-kt</property>
		</input>
		<condition>
			<and>
				<equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-throttle</value>
				</equals>
				<equals>
					<property>/autopilot/settings/kts-mach-select</property>
					<value>kts</value>
				</equals>
			</and>
		</condition>
	</label>

	<label>
		<x>-20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>KwP:%3d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/target-speed-kt</property>
		</input>
		<condition>
			<and>
				<equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-pitch-trim</value>
				</equals>
				<equals>
					<property>/autopilot/settings/kts-mach-select</property>
					<value>kts</value>
				</equals>
			</and>
		</condition>
	</label>

	<label>
		<x>-20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>MwT:%3d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/target-speed-mach</property>
		</input>
		<condition>
			<and>
				<equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-throttle</value>
				</equals>
				<equals>
					<property>/autopilot/settings/kts-mach-select</property>
					<value>mach</value>
				</equals>
			</and>
		</condition>
	</label>

	<label>
		<x>-20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>MwP:%3d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/target-speed-mach</property>
		</input>
		<condition>
			<and>
				<equals>
					<property>/autopilot/locks/speed</property>
					<value>speed-with-pitch-trim</value>
				</equals>
				<equals>
					<property>/autopilot/settings/kts-mach-select</property>
					<value>mach</value>
				</equals>
			</and>
		</condition>
	</label>

	<label>
		<x>20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>V/S:%4d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/vertical-speed-fpm</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/altitude</property>
				<value>vertical-speed-hold</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>P:%4.1f</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/target-pitch-deg</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/altitude</property>
				<value>pitch-hold</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>A:%4.1f</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/target-aoa-deg</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/altitude</property>
				<value>aoa-hold</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>%5d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/target-altitude-ft</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/altitude</property>
				<value>altitude-hold</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>AGL%4d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/target-agl-ft</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/altitude</property>
				<value>agl-hold</value>
			</equals>
		</condition>
	</label>

	<label>
		<x>20</x>
		<y>38</y>
		<width>40</width>
		<height>8</height>
		<box>true</box>
		<format>GS1</format>
		<halign>left</halign>
		<!-- input>
			<property>/autopilot/settings/target-pitch-deg</property>
		</input -->
		<condition>
			<equals>
				<property>/autopilot/locks/altitude</property>
				<value>gs1-hold</value>
			</equals>
		</condition>
	</label>

	<dial>
		<name>wind_direction</name>
		<x>75</x>
		<y>58</y> 
		<width>10</width>
		<height>10</height>
		<input>
			<property>/voodoomaster/weather/relative-wind</property>
			<factor>1.0</factor>
			<damp>0.3</damp>
		</input>
		<format></format>
		<box>false</box>
		<halign>left</halign>
		<options>0</options>
		<option>notext</option>
		<!-- justification>0</justification -->
		<!-- blinking>0</blinking -->
		<!-- working>true</working -->
		<!-- latitude>true</latitude -->
		<!-- longitude>true</longitude -->
		<!-- tick-type>line</tick-type -->
		<!-- tick-bottom>true</tick-bottom -->
		<!-- tick-left>true</tick-left -->
		<!-- cap-bottom>true</cap-bottom -->
		<display-span>0</display-span>
		<radius>8</radius>
		<divisions>10</divisions>
	</dial>

